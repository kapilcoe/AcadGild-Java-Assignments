package oops2;

public class ReverseString {
//print reverse of a string
	public static void main(String[] args) {
		String name = "ACADGILD";
		String reverse = null;
		int end = name.length();
		for(int i=0; i<end;i++)
		{
			reverse.charAt(end-i-1)=name.charAt(i);
			reverse[end-i-1]=name.charAt(i);
		}
	}
}

package oops2;

public class Assignment3 {

}



class Area{
	
	int base;
	int height;
	
	public float calculateArea(){
	return 0;
}
}


class Rectangle extends Area{
	
	public float calculateArea(){
		return base*height;
	}
}

class Triangle extends Area{
	
	public float calculateArea(){
		return (float) (0.5*base*height);
	}
}


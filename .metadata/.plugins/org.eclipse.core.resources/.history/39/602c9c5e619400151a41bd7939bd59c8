package abstractInterfaceExceptionHandling;

abstract class Student{
	String name;
	int std;
	int marks;
	char grade;
	
	abstract void calculateGrade();
}

class Info extends Student{
	
	void calculateGrade(){
		if(marks>90){
			grade = 'A';
		}else{
			grade = 'B';
		}
	}
	
}
public class Assignment2 {

}
